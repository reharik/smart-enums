{
  "name": "smart-enums",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc",
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "postbuild": "cp dist/esm/index.js dist/index.js && cp dist/cjs/index.js dist/index.cjs && cp dist/esm/index.d.ts dist/index.d.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run postbuild",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint ."
  },
  "jest-junit": {
    "suiteName": "jest: smart-enums",
    "filePathPrefix": ""
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^60.0.0",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "case-anything": "^3.1.2"
  }
}
